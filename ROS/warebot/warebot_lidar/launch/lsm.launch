<?xml version="1.0"?>
<launch>
  <include file="$(find warebot_description)/launch/filter.launch"></include>

  <!-- LIDAR odometry-->
  <node pkg="laser_scan_matcher" type="laser_scan_matcher_node" name="laser_scan_matcher_node" output="screen">
    <param name="fixed_frame" value = "odom"/>
    <param name="base_frame" value = "chassis"/>
    <param name="publish_TF" value = "true"/>

    <param name="use_odom" value = "true"/>

    <param name="kf_dist_linear" value = "0.1"/> <!--default: 0.1 -->
    <param name="kf_dist_angular" value = "0.175"/>

    <param name="max_iterations" value="40"/> <!--default: 10 -->
</node>

</launch>